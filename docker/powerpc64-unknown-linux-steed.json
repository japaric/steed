{
    "arch": "powerpc64",
    "cpu": "ppc64",
    "data-layout": "E-m:e-i64:64-n32:64",
    "env": "steed",
    "executables": true,
    "has-elf-tls": true,
    "linker-flavor": "gcc",
    "llvm-target": "powerpc64-unknown-linux",
    "max-atomic-width": 64,
    "os": "linux",
    "panic-strategy": "abort",
    "pre-link-args": {
        "gcc": [
            "-Wl,--as-needed",
            "-Wl,-z,noexecstack",
            "-m64",
            "-nostartfiles"
        ]
    },
    "target-c-int-width": "32",
    "target-endian": "big",
    "target-family": "unix",
    "target-pointer-width": "64",
    "vendor": "unknown"
}
